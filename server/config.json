{
    "data-to-include":{
        "amdgpu-pci-0100":{
            "fan1":{
                "fan1_input":{
                    "include": true,
                    "unit": "RPM",
                    "alias": "GPU fan"
                }
            },
            "edge":{
                "temp1_input":{
                    "include": true,
                    "unit": "°C",
                    "alias": "GPU temp"
                }
            }
        },
        "coretemp-isa-0000":{
            "Package id 0":{
                "temp1_input":{
                    "include": true,
                    "unit": "°C",
                    "alias": "CPU temp"
                }
            }
        }
    },
    "settings":{
        "cut_fractal": true,
        "timeout": 1,
        "address": "http://127.0.0.1:1500/"
    }

}
